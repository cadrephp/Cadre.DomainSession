{
    "name": "cadre/domain-session",
    "description": "Library for managing session data within the domain",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6.4",
        "pds/skeleton": "1.x-dev",
        "phing/phing": "^2.15",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0@RC",
        "phpstan/phpstan": "^0.6.3",
        "monolog/monolog": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Cadre\\DomainSession\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cadre\\DomainSession\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Cadre.DomainSession Contributors",
            "homepage": "https://github.com/cadrephp/Cadre.DomainSession/contributors"
        }
    ],
    "minimum-stability": "stable"
}
